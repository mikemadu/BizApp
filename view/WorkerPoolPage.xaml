<Page x:Class="BizApp.view.WorkerPoolPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BizApp.view"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="419.205"
      Title="WorkerPoolPage">

    <Grid>
        <ListBox x:Name="listBox" ItemsSource="{Binding ApplicantList}" SelectedItem="{Binding SelectedApplicant}" HorizontalAlignment="Left" Height="136" Margin="10,27,0,0" VerticalAlignment="Top" Width="121"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="10,176,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="121"/>
        <TextBox x:Name="textBox_Copy" HorizontalAlignment="Left" Height="23" Margin="10,205,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="121"/>
    </Grid>
</Page>
